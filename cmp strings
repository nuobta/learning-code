DATA SEGMENT
     MESS1 DB 'COMPUTER SOFTWARE $'
     MESS2 DB 'COMPUDER SOFTWARE $'
DATA ENDS
CODE SEGMENT
ASSUME CS:CODE, DS:DATA
START:
MOV AX,DATA
MOV DS,AX
MOV ES,AX
LEA SI,MESS1
LEA DI,MESS2
MOV CX,19
CLD
REPE CMPSB
JZ YES
MOV DL,'N'
JMP DISP
YES:MOV DL ,'Y'
DISP:MOV AH,2
INT 21H
MOV AH,4CH
INT 21H
CODE ENDS
END START
